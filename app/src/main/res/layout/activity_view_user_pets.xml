<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ViewUserPets">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/imageView"
            android:layout_width="410dp"
            android:layout_height="247dp"
            android:layout_marginTop="-15dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/rectangle_1" />

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="404dp"
            android:layout_height="164dp"
            app:layout_constraintTop_toTopOf="@+id/textView6"
            app:srcCompat="@drawable/cover"
            tools:layout_editor_absoluteX="3dp" />

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="367dp"
            android:layout_height="50dp"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView14"
            app:srcCompat="@drawable/tab_bars__auto_width___1_" />

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="91dp"
            android:layout_height="94dp"
            android:layout_marginStart="24dp"
            android:layout_marginEnd="24dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView"
            app:layout_constraintHorizontal_bias="0.501"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/textView6"
            app:srcCompat="@drawable/ellipse_1" />

        <TextView
            android:id="@+id/textView6"
            android:layout_width="112dp"
            android:layout_height="35dp"
            android:layout_marginTop="12dp"
            android:fontFamily="sans-serif"
            android:strokeColor="#FFff0000"
            android:text="Dogg Father"
            android:textColor="#474747"
            android:textSize="20sp"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toTopOf="@+id/imageView" />

        <Button
            android:id="@+id/button"
            android:layout_width="89dp"
            android:layout_height="37dp"
            android:layout_marginStart="4dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:backgroundTint="#F9F9F9"
            android:backgroundTintMode="add"
            android:fontFamily="sans-serif-black"
            android:text="My Pets"
            android:textAlignment="viewStart"
            android:textAllCaps="false"
            android:textColor="#3F51B5"
            android:textSize="12sp"
            android:textStyle="bold"
            app:iconTint="#2196F3"
            app:iconTintMode="add"
            app:layout_constraintStart_toStartOf="@+id/imageView7"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:strokeColor="#000000" />

        <Button
            android:id="@+id/button2"
            android:layout_width="87dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:backgroundTint="#F9F9F9"
            android:backgroundTintMode="add"
            android:fontFamily="sans-serif-black"
            android:text="My GIGs"
            android:textAlignment="viewStart"
            android:textAllCaps="false"
            android:textColor="#FF5722"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="visible"
            app:iconTint="#404040"
            app:iconTintMode="add"
            app:layout_constraintStart_toEndOf="@+id/button"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:strokeColor="#000000" />

        <Button
            android:id="@+id/button3"
            android:layout_width="103dp"
            android:layout_height="36dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:backgroundTint="#F9F9F9"
            android:backgroundTintMode="add"
            android:fontFamily="sans-serif-black"
            android:text="Favourites"
            android:textAlignment="viewStart"
            android:textAllCaps="false"
            android:textColor="#FF5722"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="visible"
            app:iconTint="#404040"
            app:iconTintMode="add"
            app:layout_constraintStart_toEndOf="@+id/button2"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:strokeColor="#000000" />

        <Button
            android:id="@+id/button4"
            android:layout_width="94dp"
            android:layout_height="38dp"
            android:layout_marginTop="8dp"
            android:background="@android:color/transparent"
            android:backgroundTint="#F9F9F9"
            android:backgroundTintMode="add"
            android:fontFamily="sans-serif-black"
            android:text="Requests"
            android:textAlignment="viewStart"
            android:textAllCaps="false"
            android:textColor="#FF5722"
            android:textSize="12sp"
            android:textStyle="bold"
            android:visibility="visible"
            app:iconTint="#404040"
            app:iconTintMode="add"
            app:layout_constraintEnd_toEndOf="@+id/imageView7"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/button3"
            app:layout_constraintTop_toTopOf="@+id/imageView7"
            app:strokeColor="#000000" />

        <Button
            android:id="@+id/button5"
            android:layout_width="82dp"
            android:layout_height="30dp"
            android:layout_marginStart="164dp"
            android:layout_marginEnd="164dp"
            android:backgroundTint="#FF5722"
            android:text="Edit Profile"
            android:textAlignment="center"
            android:textAllCaps="false"
            android:textSize="8sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/imageView"
            app:layout_constraintTop_toBottomOf="@+id/imageView9"
            app:strokeColor="#000000" />

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="79dp"
            android:layout_height="86dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView9"
            app:layout_constraintEnd_toEndOf="@+id/imageView9"
            app:layout_constraintStart_toStartOf="@+id/imageView9"
            app:layout_constraintTop_toTopOf="@+id/imageView9"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/dp1" />


    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/fragmentContainerView"
        android:name="com.example.petpal.UserPetView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout3"
        tools:layout="@layout/fragment_user_pet_view" />

</androidx.constraintlayout.widget.ConstraintLayout>